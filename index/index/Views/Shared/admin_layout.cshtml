<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keywords"
          content="wrappixel, admin dashboard, html css dashboard, web dashboard, bootstrap 5 admin, bootstrap 5, css3 dashboard, bootstrap 5 dashboard, Xtreme lite admin bootstrap 5 dashboard, frontend, responsive bootstrap 5 admin template, Xtreme admin lite design, Xtreme admin lite dashboard bootstrap 5 dashboard template">
    <meta name="description"
          content="Xtreme Admin Lite is powerful and clean admin dashboard template, inpired from Bootstrap Framework">
    <meta name="robots" content="noindex,nofollow">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="canonical" href="https://www.wrappixel.com/templates/xtreme-admin-lite/" />
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="@Url.Content("~/Content/admin/admin-assets/assets/images/favicon.png")">
    <!-- Custom CSS -->
    <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/chartist/dist/chartist.min.js")"></script>
    <link href="@Url.Content("~/Content/admin/admin-assets/assets/libs/chartist/dist/chartist.min.css")" rel="stylesheet" />    <!-- Custom CSS -->

    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>

    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/style.min.css")" rel="stylesheet" />
    @*<link href="@Url.Content("~/Content/admin/admin-assets/src/scss/bootstrap/_)"*@
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/style.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fa-brands.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fa-brands.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fa-regular.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fa-regular.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fa-solid.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fa-solid.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fontawesome-all.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fontawesome-all.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fontawesome.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/font-awesome/css/fontawesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/material-design-iconic-font/css/material-design-iconic-font.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/material-design-iconic-font/css/material-design-iconic-font.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/material-design-iconic-font/css/materialdesignicons.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/themify-icons/themify-icons.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/themify-icons/ie7/ie7.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/themify-icons/ie7/ie7.js")"></script>
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/style.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/style.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/weather-icons/css/weather-icons-core.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/weather-icons/css/weather-icons-variables.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/weather-icons/css/weather-icons-wind.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/weather-icons/css/weather-icons-wind.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/weather-icons/css/weather-icons.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/assets/libs/chartist/dist/chartist.min.css")" rel="stylesheet"/>
    <link href="@Url.Content("~/Content/admin/admin-assets/dist/css/icons/weather-icons/css/weather-icons.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/admin/admin-assets/assets/libs/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.css")" rel="stylesheet" />

</head>
<body>

    <div id="main-wrapper" data-layout="vertical" data-navbarbg="skin5" data-sidebartype="full" data-sidebar-position="absolute" data-header-position="absolute" data-boxed-layout="full">

        @Html.Partial("Partial/Header/adminHeader")

        <aside class="left-sidebar" data-sidebarbg="skin6">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <!-- User Profile-->
                        <li>
                            <!-- User Profile-->
                            <div class="user-profile d-flex no-block dropdown m-t-20">
                                <div class="user-pic">
                                    <img src="@Url.Content("~/Content/admin/admin-assets/assets/images/users/1.jpg")" alt="users"
                                         class="rounded-circle" width="40" />
                                </div>
                                <div class="user-content hide-menu m-l-10">
                                    <a href="#" class="" id="Userdd" role="button"
                                       data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        <h5 class="m-b-0 user-name font-medium">
                                            Steave Jobs <i class="fa fa-angle-down"></i>
                                        </h5>
                                        <span class="op-5 user-email">varun@gmail.com</span>
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-end" aria-labelledby="Userdd">
                                        <a class="dropdown-item" href="javascript:void(0)">
                                            <i class="ti-user m-r-5 m-l-5"></i> My Profile
                                        </a>
                                        <a class="dropdown-item" href="javascript:void(0)">
                                            <i class="ti-wallet m-r-5 m-l-5"></i> My Balance
                                        </a>
                                        <a class="dropdown-item" href="javascript:void(0)">
                                            <i class="ti-email m-r-5 m-l-5"></i> Inbox
                                        </a>
                                        <div class="dropdown-divider"></div>
                                        <a class="dropdown-item" href="javascript:void(0)">
                                            <i class="ti-settings m-r-5 m-l-5"></i> Account Setting
                                        </a>
                                        <div class="dropdown-divider"></div>
                                        <a class="dropdown-item" href="javascript:void(0)">
                                            <i class="fa fa-power-off m-r-5 m-l-5"></i> Logout
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <!-- End User Profile-->
                        </li>
                        <li class="p-15 m-t-10">
                            <a href=@Url.Action("Create", "Admin")
                               class="btn d-block w-100 create-btn text-white no-block d-flex align-items-center">
                                <i class="fa fa-plus-square"></i> <span class="hide-menu m-l-5"> Add New Item</span>
                            </a>
                        </li>
                        <!-- User Profile-->
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               href=@Url.Action("Index", "Admin") aria-expanded="false">
                                <i class="mdi mdi-view-dashboard"></i><span class="hide-menu">Items</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               href=@Url.Action("SubItem", "Admin") aria-expanded="false">
                                <i class="mdi mdi-account-network"></i><span class="hide-menu">Sub Item</span>
                            </a>
                        </li>

                    </ul>

                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>

        <div class="page-wrapper">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>

        @Html.Partial("Partial/Footer/adminFooter")

    </div>




            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/jquery/dist/jquery.min.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/bootstrap/dist/js/bootstrap.bundle.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/bootstrap/dist/js/bootstrap.esm.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/bootstrap/dist/js/bootstrap.esm.min.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/bootstrap/dist/js/bootstrap.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/bootstrap/dist/js/bootstrap.min.js")"></script>
            <!-- Bootstrap tether Core JavaScript -->
            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/dist/js/waves.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/dist/js/sidebarmenu.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/dist/js/custom.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/dist/js/app-style-switcher.js")"></script>
            <!--chartis chart-->
            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/chartist/dist/chartist.min.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/assets/libs/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.min.js")"></script>
            <script src="@Url.Content("~/Content/admin/admin-assets/dist/js/pages/dashboards/dashboard1.js")"></script>

</body>
</html>
